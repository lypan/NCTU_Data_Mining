param = {
   'objective':'multi:softmax',
   'eta':0.3,
   'max_depth':24,
   'num_class':20,
   'colsample_bytree':0.3,
   'min_child_weight':1
}
num_rounds = 100

clf = xgb.train(param, xgb.DMatrix(dtm_train, cuisine_label), num_rounds)


xgb.cv(param, xgb.DMatrix(dtm_train, cuisine_label), num_rounds, nfold=5,
       metrics={'merror'}, seed = 0)
